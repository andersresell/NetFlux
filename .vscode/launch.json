{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "runNetFluxBrickSmall",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debug/NetFlux",
            "args": [
                "${workspaceFolder}/testing/netflux_tests/small_brick_domain"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/src/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build debug NetFlux"
        },
